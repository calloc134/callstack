{
  "name": "backend",
  "version": "0.0.1",
  "type": "commonjs",
  "scripts": {
    "dev": "NODE_ENV=development tsx watch ./src/index.ts",
    "build": "swc src -d dist/src && tsc-alias",
    "start": "NODE_ENV=production node ./dist/src/index.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "codegen": "graphql-codegen --config codegen.ts",
    "prigen": "prisma generate",
    "primig": "prisma migrate deploy",
    "priseed": "prisma db seed"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@envelop/auth0": "^5.0.0",
    "@envelop/core": "^4.0.0",
    "@envelop/disable-introspection": "^5.0.0",
    "@envelop/generic-auth": "^6.0.0",
    "@envelop/graphql-jit": "^6.0.1",
    "@escape.tech/graphql-armor": "^2.2.0",
    "@graphql-tools/load-files": "^7.0.0",
    "@graphql-tools/merge": "^9.0.0",
    "@prisma/client": "^5.1.1",
    "@swc/helpers": "^0.5.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/uuid": "^9.0.3",
    "graphql": "^16.7.1",
    "graphql-middleware": "^6.1.35",
    "graphql-scalars": "^1.22.2",
    "graphql-shield": "^7.6.5",
    "graphql-yoga": "^4.0.3",
    "jimp": "^0.22.10",
    "jsonwebtoken": "^9.0.1",
    "jwks-rsa": "^3.0.1",
    "minio": "^7.1.3",
    "prisma": "^5.1.1",
    "ts-patch": "^3.0.2",
    "tsc-alias": "^1.8.7",
    "tsx": "^3.12.7",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/typescript": "4.0.1",
    "@graphql-codegen/typescript-resolvers": "4.0.1",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.74",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.46.0",
    "typescript": "^5.1.6"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
